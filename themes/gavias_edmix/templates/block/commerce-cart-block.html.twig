<div class="title"><a class="link-qc" href="javascript:void(0);"><span class="icon gv-icon-924"></span><span class="cart-count">{{ count }}</span></a></div>
<div class="content-inner">
  {% if content %}
  <div class="cart-contents">
    <div class="cart-block-contents-inner">
      <div class="cart-block-contents-items">
        {{ content }}
      </div>
      <div class="cart-block-contents-links">
        {{ links }}
      </div>
    </div>
  </div>
  {% else %}
    <div class="cart-no-items">
      <p>{{'Your cart is empty.'|t}}</p>
      <p><strong><a href="{{path('<front>')}}"><span class="text-theme">{{'Keep Shopping'|t}}</span></a></strong></p>
    </div>
  {% endif %}
</div>